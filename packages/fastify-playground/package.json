{
  "name": "@mypub/fastify-playground",
  "version": "0.0.0",
  "type": "module",
  "description": "MyPub Fastify Plugin Playground",
  "private": true,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "check:types": "tsc --noEmit",
    "dev": "sleep 2; NODE_ENV=debug nodemon -e js,ts --watch '..' --delay 1 --exec ts-node ./src/main.ts",
    "lint": "eslint --cache --cache-location \".cache/eslintcache\" src && prettier --check src"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "packageManager": "pnpm@7.18.2",
  "pnpm": {},
  "dependencies": {
    "@mypub/fastify": "workspace:*",
    "@mypub/memory": "workspace:*",
    "@mypub/mongodb": "workspace:*",
    "@mypub/s3": "workspace:*",
    "@mypub/single-user": "workspace:*",
    "dotenv": "^16.0.3",
    "fastify": "^4.10.2",
    "mypub": "workspace:*",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@mypub/types": "workspace:*",
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "pino-pretty": "^9.1.1",
    "typescript": "^4.9.4"
  }
}
